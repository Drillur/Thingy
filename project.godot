config_version=5

[application]

config/name="Thingy"
config/version="0.1.1"
run/main_scene="res://Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Thingy"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://Art/Icons/Icon.png"
config/windows_native_icon="res://Art/Icons/Icon.ico"

[autoload]

bag="*res://Scripts/Autoload/Resources.gd"
tp="*res://Scripts/Autoload/TimeParser.gd"
SaveManager="*res://Scripts/Autoload/SaveManager.gd"
gv="*res://Scripts/Autoload/Overseer.gd"
Settings="*res://Scripts/Autoload/Settings.gd"
wa="*res://Scripts/Autoload/Wallet.gd"
up="*res://Scripts/Autoload/Upgrades.gd"
th="*res://Scripts/Autoload/Thingies.gd"
DiscordSDKLoader="*res://addons/discord-sdk-gd/nodes/discord_autoload.gd"

[debug]

gdscript/warnings/unassigned_variable=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1408
window/size/viewport_height=792
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-sdk-gd/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "red",
"res://Hud/": "orange",
"res://Scripts/": "green"
}

